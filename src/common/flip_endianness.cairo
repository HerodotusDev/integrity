use cairo_verifier::common::{from_span::FromSpanTrait, array_append::ArrayAppendTrait};

trait FlipEndiannessTrait<F> {
    fn flip_endianness(self: F) -> F;
}

impl FlipEndiannessU32 of FlipEndiannessTrait<u32> {
    fn flip_endianness(self: u32) -> u32 {
        (self % 256) * 16777216
            + (self / 256 % 256) * 65536
            + (self / 65536 % 256) * 256
            + (self / 16777216 % 256)
    }
}

